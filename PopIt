local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window  = Library.CreateLib("Script Made By Phobia", "Serpent")
local Tab     = Window:NewTab("Item Buyer")
local Section = Tab:NewSection("Item Buyer")
Section:NewButton("Buy Dart", "Buys a Dart", function()
    local args = {
        [1] = "Dart"
    }

    game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end)
